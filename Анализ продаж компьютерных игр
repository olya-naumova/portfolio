    "# Для удобства объединим некоторые рейтинги, так как они близки по возрастному ограничению\n",
    "data.loc[data['rating'] == 'RP', 'rating'] = 'не определен'\n",
    "data.loc[(data['rating'] == 'K-A') & (data['rating'] == 'EC'), 'rating'] = 'E'\n",
    "data.loc[data['rating'] == 'AO', 'rating'] = 'M'"
   ]
  },
